Private Sub Form_Load()
    Call updateCheckChart
End Sub

Public Sub updateCheckChart()
Dim i As Integer
    For i = 1 To MAXSENSOR
        If Sensor(i).Used = True Then
            Check1(i).value = Checked
        Else
            Check1(i).value = Unchecked
        End If
    Next i
End Sub



Private Sub Form_Unload(Cancel As Integer)
Dim i As Integer
    For i = 1 To MAXSENSOR
        If Check1(i).value = Checked Then
            Sensor(i).Used = True
        Else
            Sensor(i).Used = False
        End If
    Next i
End Sub

